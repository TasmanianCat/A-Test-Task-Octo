{
  "name": "a-test-task-octo",
  "version": "1.0.0",
  "description": "A test task",
  "browserslist": [
    ">0.5%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "build:sass": "sass assets/scss/styles.scss assets/css/styles.css",
    "watch:sass": "sass --watch assets/scss:assets/css",
    "build:css": "postcss assets/css/styles.css -o assets/css/styles.min.css",
    "build": "npm run build:sass && npm run build:css",
    "build:js": "rollup -c",
    "watch:js": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Sergei Serbulatov",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.97.3"
  }
}
